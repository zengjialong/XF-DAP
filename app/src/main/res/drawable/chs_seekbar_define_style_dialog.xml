<?xml version="1.0" encoding="UTF-8"?>     
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">  
    <item android:id="@android:id/background">     
      <shape>     
         <corners android:radius="10dip" />     
         <gradient   
             android:startColor="#1c74b1" 
             android:centerColor="#1c74b1"    
             android:endColor="#1c74b1"   
             android:centerY="1"    
             android:angle="270"/>     
      </shape>     
   </item>     
       
   <item android:id="@android:id/progress">     
       <clip>     
          <shape>     
              <corners android:radius="10dip" />     
              <gradient   
                  android:startColor="#24c3ff" 
                  android:centerColor="#24c3ff"    
                  android:endColor="#24c3ff"   
                  android:centerY="1"    
                  android:angle="270"/>     
          </shape>     
       </clip>     
   </item>  
      
