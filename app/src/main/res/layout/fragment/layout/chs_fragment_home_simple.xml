<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:KNOB="http://schemas.android.com/apk/res-auto"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:mhsb_seekbar="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/id_llyout_net_toning_home"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/nullc">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:fillViewport="true"
        android:gravity="center"
        android:orientation="vertical">

        <TextView
            android:id="@+id/id_tv_master_sub"
            android:layout_width="140dp"
            android:layout_height="@dimen/space_30"
            android:layout_centerInParent="true"
            android:layout_gravity="center"
            android:background="@null"
            android:clickable="false"
            android:gravity="center"
            android:text="@string/GroupOpt"
            android:textAllCaps="false"
            android:textColor="@color/main_vol_color"
            android:textSize="@dimen/space_13" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/space_80"
            android:fadingEdge="none"
            android:fillViewport="true"
            android:orientation="horizontal"
            android:overScrollMode="never"
            android:scrollbars="none">

            <Button
                android:id="@+id/id_not_in_china"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginBottom="10dp"
                android:background="@drawable/chs_btn_encryption"
                android:overScrollMode="never"
                android:text="当前你的机器被锁定因为可能不在中国大陆地区"
                android:textColor="@color/white"
                android:textSize="@dimen/space_20"
                android:visibility="gone" />

            <com.chs.mt.xf_dap.tools.MHorizontalScrollView
                android:id="@+id/id_llyout_userseff_bar"
                android:layout_width="match_parent"
                android:layout_height="@dimen/space_80"
                android:fadingEdge="none"

                android:fillViewport="true"
                android:orientation="horizontal"
                android:overScrollMode="never"
                android:scrollbars="none">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:baselineAligned="false"
                    android:fillViewport="true"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:id="@+id/ly_useg_1"
                        android:layout_width="@dimen/space_120"
                        android:layout_height="match_parent"
                        android:baselineAligned="false"
                        android:fillViewport="true"
                        android:gravity="center"

                        android:orientation="horizontal"
                        android:padding="@dimen/space_2">

                        <Button
                            android:id="@+id/btn_useg_1"
                            android:layout_width="@dimen/space_80"
                            android:layout_height="@dimen/space_30"
                            android:layout_gravity="center"
                            android:background="@drawable/chs_user_b_normal"
                            android:gravity="center"
                            android:text="第一组"
                            android:textAllCaps="false"
                            android:textColor="@color/user_group_text_color_normal"
                            android:textSize="@dimen/usergroup_button_tv_name_size" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ly_useg_2"
                        android:layout_width="@dimen/space_120"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/space_2"
                        android:layout_marginRight="@dimen/space_2"
                        android:baselineAligned="false"
                        android:fillViewport="true"
                        android:gravity="center"

                        android:orientation="horizontal"
                        android:padding="@dimen/space_2">

                        <Button
                            android:id="@+id/btn_useg_2"
                            android:layout_width="@dimen/space_80"
                            android:layout_height="@dimen/space_30"
                            android:layout_gravity="center"
                            android:background="@drawable/chs_user_b_normal"
                            android:gravity="center"
                            android:text="OFF"
                            android:textAllCaps="false"

                            android:textColor="@color/user_group_text_color_normal"
                            android:textSize="@dimen/usergroup_button_tv_name_size" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ly_useg_3"
                        android:layout_width="@dimen/space_120"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/space_2"
                        android:layout_marginRight="@dimen/space_2"
                        android:baselineAligned="false"
                        android:fillViewport="true"
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:padding="@dimen/space_2">

                        <Button
                            android:id="@+id/btn_useg_3"
                            android:layout_width="@dimen/space_80"
                            android:layout_height="@dimen/space_30"
                            android:layout_gravity="center"
                            android:background="@drawable/chs_user_b_normal"
                            android:gravity="center"

                            android:text="OFF"
                            android:textAllCaps="false"
                            android:textColor="@color/user_group_text_color_normal"
                            android:textSize="@dimen/usergroup_button_tv_name_size" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ly_useg_4"
                        android:layout_width="@dimen/space_120"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/space_2"
                        android:layout_marginRight="@dimen/space_2"
                        android:baselineAligned="false"
                        android:fillViewport="true"
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:padding="@dimen/space_2">

                        <Button
                            android:id="@+id/btn_useg_4"
                            android:layout_width="@dimen/space_80"
                            android:layout_height="@dimen/space_30"
                            android:layout_gravity="center"

                            android:background="@drawable/chs_user_b_normal"
                            android:gravity="center"

                            android:text="OFF"
                            android:textAllCaps="false"
                            android:textColor="@color/user_group_text_color_normal"
                            android:textSize="@dimen/usergroup_button_tv_name_size" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ly_useg_5"
                        android:layout_width="@dimen/space_120"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/space_2"
                        android:layout_marginRight="@dimen/space_2"
                        android:baselineAligned="false"
                        android:fillViewport="true"
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:padding="@dimen/space_2">

                        <Button
                            android:id="@+id/btn_useg_5"
                            android:layout_width="@dimen/space_80"
                            android:layout_height="@dimen/space_30"
                            android:layout_gravity="center"
                            android:background="@drawable/chs_user_b_normal"
                            android:gravity="center"
                            android:text="OFF"
                            android:textAllCaps="false"
                            android:textColor="@color/user_group_text_color_normal"
                            android:textSize="@dimen/usergroup_button_tv_name_size" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ly_useg_6"
                        android:layout_width="@dimen/space_120"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/space_2"
                        android:layout_marginRight="@dimen/space_2"
                        android:baselineAligned="false"
                        android:fillViewport="true"
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:padding="@dimen/space_2">

                        <Button
                            android:id="@+id/btn_useg_6"
                            android:layout_width="@dimen/space_80"
                            android:layout_height="@dimen/space_30"
                            android:layout_gravity="center"
                            android:background="@drawable/chs_user_b_normal"
                            android:gravity="center"
                            android:text="OFF"
                            android:textAllCaps="false"
                            android:textColor="@color/user_group_text_color_normal"
                            android:textSize="@dimen/usergroup_button_tv_name_size" />
                    </LinearLayout>

                </LinearLayout>
            </com.chs.mt.xf_dap.tools.MHorizontalScrollView>
        </RelativeLayout>

        <Button
            android:layout_width="@dimen/space_80"
            android:layout_height="@dimen/space_20"
            android:background="@color/nullc"
            android:text="@string/MainValume"
            android:textAllCaps="false"
            android:textColor="@color/main_vol_color"
            android:textSize="@dimen/space_13" />



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="5"
            android:orientation="horizontal">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">


                <com.chs.mt.xf_dap.tools.KnobViewLine_OutThumb
                    android:id="@+id/id_sbi_0"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/space_50"
                    android:layout_marginRight="@dimen/space_50"
                    android:layout_centerInParent="true"
                    android:thumb="@drawable/chs_thumb_normal"
                    mhsb_seekbar:index_num="19"
                    mhsb_seekbar:index_size="25dp"
                    mhsb_seekbar:inside_cir_color="#FF212f3b"
                    mhsb_seekbar:outside_line_color_normal="@color/red"
                    mhsb_seekbar:outside_line_color_press="@color/delay_seekbar_progress_color"
                    mhsb_seekbar:outside_line_width="12dp"
                    mhsb_seekbar:line_long="10dp"
                    mhsb_seekbar:line_width="8dp"
                    />

                <TextView
                    android:id="@+id/id_main_val_simple"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:text="40"
                    android:textColor="@color/white"
                    android:textSize="@dimen/space_60" />
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/space_50"
            android:layout_gravity="center"
            android:gravity="center"
            android:orientation="horizontal">

            <com.chs.mt.xf_dap.tools.LongCickButton
                android:id="@+id/id_button_val_sub"
                android:layout_width="@dimen/space_40"
                android:layout_height="@dimen/space_40"
                android:layout_gravity="center"
                android:layout_marginRight="@dimen/space_80"
                android:background="@drawable/chs_btn_s_sub_selector"
                android:gravity="center" />

            <LinearLayout
                android:id="@+id/id_ly_mute"
                android:layout_width="@dimen/space_40"
                android:layout_height="@dimen/space_40"
                android:layout_gravity="center"
                android:gravity="center"
                android:background="@drawable/chs_btn_mute_normal"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/id_b_mute"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_gravity="center"
                    android:src="@drawable/chs_mute_normal"
                    android:clickable="false"
                    android:gravity="center"
                    android:scaleType="fitCenter"
                    android:textAllCaps="false"
                    android:textSize="60sp" />
            </LinearLayout>
            <com.chs.mt.xf_dap.tools.LongCickButton
                android:id="@+id/id_button_val_inc"
                android:layout_width="@dimen/space_40"
                android:layout_height="@dimen/space_40"
                android:layout_gravity="center"
                android:layout_marginLeft="@dimen/space_80"
                android:background="@drawable/chs_btn_s_inc_selector"
                android:gravity="center" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/space_60"
            android:layout_gravity="center"
            android:layout_marginBottom="@dimen/space_10"
            android:gravity="center"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/id_txt_sub_title"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="@string/MSUB_VAL"
                    android:textAllCaps="false"
                    android:textColor="@color/white"
                    android:textSize="@dimen/space_11" />

                <TextView
                    android:id="@+id/id_sub_val"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="40"
                    android:textColor="@color/white"
                    android:textSize="@dimen/space_12" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"

                    android:layout_marginLeft="@dimen/space_10"
                    android:layout_marginRight="@dimen/space_10"
                    android:gravity="center"


                    android:orientation="horizontal">

                    <com.chs.mt.xf_dap.tools.LongCickButton
                        android:id="@+id/id_btn_subval_sub"
                        android:layout_width="@dimen/delay_item_button_incsub_size"
                        android:layout_height="@dimen/delay_item_button_incsub_size"
                        android:layout_gravity="center"
                        android:background="@drawable/chs_s_sub_normal"
                        android:gravity="center" />

                    <com.chs.mt.xf_dap.tools.MHS_thumb_half_SeekBar
                        android:id="@+id/id_sub_val_seekbar"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_weight="1.0"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp"
                        android:thumb="@drawable/chs_thumb_simple_normal"
                        mhsb_seekbar:mhs_progress_background_color="@color/mastervolume_seekbar_progress_bg_color"
                        mhsb_seekbar:mhs_progress_color="@color/mastervolume_seekbar_progress_color"
                        mhsb_seekbar:mhs_progress_width="2dp" />

                    <com.chs.mt.xf_dap.tools.LongCickButton
                        android:id="@+id/id_btn_subval_inc"
                        android:layout_width="@dimen/delay_item_button_incsub_size"
                        android:layout_height="@dimen/delay_item_button_incsub_size"
                        android:layout_gravity="center"
                        android:background="@drawable/chs_s_inc_normal"
                        android:gravity="center" />
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/id_txt_subfreq_title"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="@string/sub_freq"
                    android:textAllCaps="false"
                    android:textColor="@color/white"
                    android:textSize="@dimen/space_11" />

                <TextView
                    android:id="@+id/id_subfreq_val"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="40"
                    android:textAllCaps="false"
                    android:textColor="@color/white"
                    android:textSize="@dimen/space_12" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_marginLeft="@dimen/space_10"
                    android:layout_marginRight="@dimen/space_10"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <com.chs.mt.xf_dap.tools.LongCickButton
                        android:id="@+id/id_btn_subfreq_sub"
                        android:layout_width="@dimen/delay_item_button_incsub_size"
                        android:layout_height="@dimen/delay_item_button_incsub_size"
                        android:layout_gravity="center"
                        android:background="@drawable/chs_s_sub_normal"
                        android:gravity="center" />

                    <com.chs.mt.xf_dap.tools.MHS_thumb_half_SeekBar
                        android:id="@+id/id_sub_freq_seekbar"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_weight="1.0"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp"
                        android:thumb="@drawable/chs_thumb_simple_normal"
                        mhsb_seekbar:mhs_progress_background_color="@color/mastervolume_seekbar_progress_bg_color"
                        mhsb_seekbar:mhs_progress_color="@color/mastervolume_seekbar_progress_color"
                        mhsb_seekbar:mhs_progress_width="2dp" />

                    <com.chs.mt.xf_dap.tools.LongCickButton
                        android:id="@+id/id_btn_subfreq_inc"
                        android:layout_width="@dimen/delay_item_button_incsub_size"
                        android:layout_height="@dimen/delay_item_button_incsub_size"
                        android:layout_gravity="center"
                        android:background="@drawable/chs_s_inc_normal"
                        android:gravity="center" />
                </LinearLayout>
            </LinearLayout>



        </LinearLayout>
        <RelativeLayout
            android:id="@+id/id_rl_inputsource"
            android:layout_width="match_parent"
            android:layout_height="@dimen/space_50"
            android:background="@color/delay_select_bg"
            android:orientation="horizontal">

            <Button
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/space_10"
                android:background="@color/transparent"
                android:text="@string/Ad_InputSourceMain"
                android:textAlignment="viewStart"
                android:textAllCaps="false"
                android:textColor="@color/white"
                android:textSize="@dimen/space_12" />

            <Button
                android:id="@+id/id_btn_inputsource"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/space_15"
                android:background="@color/nullc"
                android:paddingRight="@dimen/space_10"
                android:textAlignment="viewEnd"
                android:textAllCaps="false"
                android:textColor="@color/white"
                android:textSize="@dimen/space_12" />
            <ImageView
                android:layout_width="@dimen/space_10"
                android:layout_height="@dimen/space_10"
                android:layout_gravity="center"
                android:gravity="center"
                android:textColor="@color/white"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/space_5"
                android:src="@drawable/chs_delay_back"
                android:layout_centerVertical="true"
                android:textSize="@dimen/space_12"
                />
        </RelativeLayout>


    </LinearLayout>


</RelativeLayout>