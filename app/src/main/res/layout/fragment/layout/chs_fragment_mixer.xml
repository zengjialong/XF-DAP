<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:wheelview="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            >
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/space_45">
                <!-- 通道选择-->
                <com.chs.mt.xf_dap.tools.wheel.WheelView
                    android:id="@+id/id_output_va_wheelview"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/space_45"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/space_5"
                    android:background="@color/wheelView_bg"
                    wheelview:fontColor="@color/white"
                    wheelview:fontColor_press="@color/text_color_TopBarChannelSel_press"
                    wheelview:fontSize="@dimen/text_size_TopBarChannelSel" />
                <View
                    android:layout_width="@dimen/space_45"
                    android:layout_height="3dp"
                    android:layout_gravity="center"
                    android:layout_centerHorizontal="true"
                    android:layout_alignParentBottom="true"
                    android:background="@color/channelsel_btn_stroke_color_press" />
            </RelativeLayout>

            <com.chs.mt.xf_dap.tools.MScrollView
                android:id="@+id/id_weight_srollview"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:fillViewport="true"
                android:overScrollMode="never"
                android:scrollbars="none"
                android:fadingEdge="none"
                android:layout_gravity="center"
                android:orientation="vertical">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fillViewport="true"
                    android:gravity="center"
                    android:orientation="vertical"
                    >
                    <com.chs.mt.xf_dap.viewItem.V_MixerItem
                        android:id="@+id/id_mixer_0"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/space_120"/>
                    <com.chs.mt.xf_dap.viewItem.V_MixerItem
                        android:id="@+id/id_mixer_1"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/space_120"/>
                    <com.chs.mt.xf_dap.viewItem.V_MixerItem
                        android:id="@+id/id_mixer_2"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/space_120"/>
                    <com.chs.mt.xf_dap.viewItem.V_MixerItem
                        android:id="@+id/id_mixer_3"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/space_120"/>
                    <com.chs.mt.xf_dap.viewItem.V_MixerItem
                        android:id="@+id/id_mixer_4"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/space_120"/>
                    <com.chs.mt.xf_dap.viewItem.V_MixerItem
                        android:id="@+id/id_mixer_5"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/space_120"/>
                    <com.chs.mt.xf_dap.viewItem.V_MixerItem
                        android:id="@+id/id_mixer_6"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/space_120"/>
                    <com.chs.mt.xf_dap.viewItem.V_MixerItem
                        android:id="@+id/id_mixer_7"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/space_120"/>
                    <com.chs.mt.xf_dap.viewItem.V_MixerItem
                        android:id="@+id/id_mixer_8"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/space_120"/>
                    <com.chs.mt.xf_dap.viewItem.V_MixerItem
                        android:id="@+id/id_mixer_9"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/space_120"/>
                    <com.chs.mt.xf_dap.viewItem.V_MixerItem
                        android:id="@+id/id_mixer_10"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/space_120"/>
                    <com.chs.mt.xf_dap.viewItem.V_MixerItem
                        android:id="@+id/id_mixer_11"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/space_120"/>
                    <com.chs.mt.xf_dap.viewItem.V_MixerItem
                        android:id="@+id/id_mixer_12"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/space_120"/>
                    <com.chs.mt.xf_dap.viewItem.V_MixerItem
                        android:id="@+id/id_mixer_13"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/space_120"/>
                    <com.chs.mt.xf_dap.viewItem.V_MixerItem
                        android:id="@+id/id_mixer_14"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/space_120"/>
                    <com.chs.mt.xf_dap.viewItem.V_MixerItem
                        android:id="@+id/id_mixer_15"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/space_120"/>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="10dp"
                        />

                </LinearLayout>
            </com.chs.mt.xf_dap.tools.MScrollView>
            <include
                android:id="@+id/id_channel_sel_mixer"
                layout="@layout/chs_channel_sel"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="@dimen/space_120"/>
        </LinearLayout>
        <Button
            android:id="@+id/id_b_encryption"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="@string/HadEncryption"
            android:textColor="@color/text_color_red"
            android:textSize="35sp"
            android:textAllCaps="false"
            android:visibility="gone"
            android:background="@drawable/chs_btn_encryption"/>


    </RelativeLayout>

</FrameLayout>
